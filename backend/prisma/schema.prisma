generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  firstName  String   @map("first_name")  
  lastName   String   @map("last_name")   
  email      String   @unique             
  password   String                        
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}